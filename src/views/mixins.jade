mixin traverseRoutes(obj)
        each val, key in obj
            if(key === "path")
                li
                    <a href="/#{obj['language']}/#{obj['url']}" class="#{obj['name']}">#{obj['title']}</a>
                    if(obj['_routes'].length > 0)
                        ul
                            each val in obj['_routes']
                                +traverseRoutes(val)